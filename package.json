{
  "name": "web-automation-electron",
  "main": "dist/electron/main.js",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "electron ./dist/electron/main.js",
    "build:client": "tsc && vite build",
    "build:main": "tsc --project tsconfig.electron.json",
    "build": "yarn build:client && yarn build:main",
    "dev:client": "vite",
    "dev:main": "tsc-watch -p tsconfig.electron.json --onSuccess \"yarn start:electron\"",
    "package": "yarn build && electron-builder",
    "preview": "vite preview",
    "start:electron": "NODE_ENV=development electron ./dist/electron/main.js",
    "start:main": "yarn build:main && yarn start:electron"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^2.0.0-next.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "framer-motion": "^6.3.3",
    "immer": "^9.0.12",
    "nanoid": "^3.3.4",
    "playwright": "^1.21.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.7"
  },
  "build": {
    "appId": "com.jungdujang.web-automation",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "packaged"
    },
    "publish": null
  }
}
