{
  "name": "web-automation-electron",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start:main": "yarn build:main && yarn start:electron",
    "start:electron": "NODE_ENV=development electron ./dist/main.js",
    "dev:electron": "tsc-watch --noClear -p tsconfig.electron.json --onSuccess \"yarn start:electron\"",
    "dev:client": "vite",
    "build": "yarn build:client && yarn build:main",
    "build:client": "tsc && vite build",
    "build:main": "tsc --project tsconfig.electron.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "electron": "^18.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.7"
  }
}
